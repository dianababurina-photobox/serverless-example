{
  "name": "@photobox/photo-services-hello-world",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "scripts": {
    "start": "yarn build && sls offline start",
    "start:watch": "nodemon --exec yarn start",
    "cleanup": "rimraf dist/",
    "build": "yarn build:prod",
    "build:dev": "yarn cleanup && tsc -p tsconfig.json",
    "build:prod": "yarn cleanup && tsc -p tsconfig-prod.json",
    "deploy:feature": "yarn build && sls deploy -v --stage feature",
    "deploy:development": "yarn build && sls deploy -v --stage development",
    "deploy:staging": "yarn build && sls deploy -v --stage staging",
    "deploy:production": "yarn build && sls deploy -v --stage production",
    "prettier:check": "prettier 'src/**/*.{js,ts}' --check",
    "lint": "eslint src/** --ext .ts",
    "lint:fix": "eslint src/** --ext .ts --fix",
    "preBuild": "tsc",
    "test": "jest --verbose ./src",
    "test:debug": "node --inspect-brk ../..node_modules/.bin/jest -i",
    "test:watch": "jest --watch",
    "hook:precommit": "lint-staged",
    "hook:prepush": "yarn build && lint-staged && yarn test --passWithNoTests",
    "local:install-db": "sls dynamodb install -s local",
    "local:remove-db": "sls dynamodb remove -s local",
    "local:start-db": "sls dynamodb start -s local",
    "local:start": "sls offline start -s local"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn hook:precommit",
      "pre-push": "yarn hook:prepush"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint  --ext .ts --ext .js --fix"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.39",
    "@types/aws-sdk": "^2.7.0",
    "@types/config": "^0.0.36",
    "@types/jest": "^24.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/serverless": "^1.78.26",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "aws-sdk-mock": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cpy-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": ">=24.0.0 <25.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "serverless": "^1.61.1",
    "serverless-deployment-bucket": "^1.1.2",
    "serverless-domain-manager": "^3.3.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^5.12.1",
    "serverless-offline-aws-sqs": "^1.0.3",
    "serverless-offline-scheduler": "^0.3.8",
    "serverless-offline-sns": "^0.68.0",
    "serverless-offline-sqs": "^3.1",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^24.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@middy/core": "^2.2.0",
    "@middy/input-output-logger": "^2.2.0",
    "serverless-prune-plugin": "^1.5.0"
  }
}
